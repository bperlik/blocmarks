  <h1>Edit Bookmark</h1>
    <div class="row">
      <div class="col-md-4">
      <h1><%= @bookmark.url %></h1>
        <div class="col-md-8">
          <%= form_for [@topic, @bookmark] do |f| %>
            <div class="form-group">
              <%= f.label :URL %>
              <%= f.text_field :url, class: 'form-control', placeholder: "Enter URL" %>
            </div>
          <%= f.submit "Save", class: 'btn btn-success' %>
        </div>
      <% end %>
    </div>
  </div>
