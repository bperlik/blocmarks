<h1>All Likes <h1>

<div class="row">
  <div class="col-md-8">
   <% @topics.each do |topic| %>
     <div class="media">
       <div class="media-body">
         <% liked_bookmarks.each do |bookmark| %>
           <h4 float-right>
               <%= link_to "#{liked_bookmark.url}", topic_bookmark_path(liked_bookmark.topic, liked_bookmark) %>
           </h4>
           <br>
         <% end %>
       </div>
     </div>
     <% end %>
  </div>
 </div>

